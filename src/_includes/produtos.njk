<section id="produtos" class="section produtos">
  <h2 class="title">{{ produtos.titulo }}</h2>

  <div class="container">
      <div class="produto">
        {% for produto in produtos.produtos %}
          {{ produto.nome }}
          {% for imagem in produto.imagens %}
            {{ imagem.url }}
            {{ imagem.descricao }}
          {% endfor %}   
        {% endfor %}   
      </div>
  </div>
</section>